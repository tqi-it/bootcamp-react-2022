{
  "compilerOptions": {
    "jsx": "react",
    "target": "es6",
    "baseUrl": "src",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "components/*": ["./components/*"],
      "containers/*": ["./containers/*"],
      "assets/*": ["./assets/*"],
      "service/*": ["./service/*"],
      "routes/*": ["./routes/*"],
      "commons/*": ["./commons/*"]
    }
  },
  "moduleResolution": "classic",
  "exclude": ["node_modules", "**/node_modules/*", "build"]
}
